{
  "name": "@runwayml/avatar-react",
  "version": "0.1.0",
  "description": "React SDK for real-time AI avatar interactions with GWM-1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./widget": {
      "import": "./dist/widget/index.js",
      "require": "./dist/widget/index.global.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:widget": "tsup --config tsup.config.ts src/widget/index.ts",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "runwayml",
    "avatar",
    "ai",
    "react",
    "hooks",
    "real-time",
    "gwm-1",
    "video",
    "streaming"
  ],
  "author": "RunwayML",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runwayml/avatar-sdk-react.git"
  },
  "bugs": {
    "url": "https://github.com/runwayml/avatar-sdk-react/issues"
  },
  "homepage": "https://github.com/runwayml/avatar-sdk-react#readme",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@livekit/components-react": "^2.8.2",
    "livekit-client": "^2.7.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/bun": "^1.3.7",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "jsdom": "^27.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  }
}
